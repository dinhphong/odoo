<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <template id="onepay_cc_form">
            <div>
                <input type="hidden" name="data_set" t-att-data-action-url="tx_url" data-remove-me=""/>
                <input type="hidden" name="vpc_Version" t-att-value="vpc_Version"/>
                <input type="hidden" name="vpc_Currency" t-att-value="vpc_Currency"/>
                <input type="hidden" name="vpc_Command" t-att-value="vpc_Command" />
                <input type="hidden" name="vpc_AccessCode" t-att-value="vpc_AccessCode"/>
                <input type="hidden" name="vpc_Merchant" t-att-value="vpc_Merchant"/>
                <input type="hidden" name="vpc_Locale" t-att-value="vpc_Locale"/>
                <input t-if="handling" type="hidden" name="handling"
                    t-att-value="handling"/>
                <input type="hidden" name="vpc_ReturnURL" t-att-value="vpc_ReturnURL"/>
                <!-- partner / address data -->
                <input type="hidden" name="vpc_MerchTxnRef" t-att-value="vpc_MerchTxnRef"/>
                <input type="hidden" name="vpc_OrderInfo" t-att-value="vpc_OrderInfo"/>
                <input type="hidden" name="vpc_Amount" t-att-value="vpc_Amount"/>
                <input type="hidden" name="vpc_TicketNo" t-att-value="vpc_TicketNo"/>
                <input type="hidden" name="AgainLink" t-att-value="AgainLink"/>
                <input type="hidden" name="Title" t-att-value="Title"/>
                <input type="hidden" name="vpc_SecureHash" t-att-value="vpc_SecureHash"/>
                <input type="hidden" name="vpc_Customer_Phone" t-att-value="vpc_Customer_Phone"/>
                <input type="hidden" name="vpc_Customer_Email" t-att-value="vpc_Customer_Email"/>
                <input type="hidden" name="vpc_Customer_Id" t-att-value="vpc_Customer_Id"/>
            </div>
        </template>
    </data>
</odoo>
